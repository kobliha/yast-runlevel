/**
 * YaST2 test case for runlevel editor
 *
 * tests function dirtyServices
 */
{
    include "testsuite.ycp";
    import "Runlevel";

    Runlevel::services = $[
        "a" : $[ ],
        "b" : $["start" : [ "1", ]],
        ];
    DUMP (Runlevel::isDirty ());
    Runlevel::services = $[
        "a" : $["dirty" : true ],
        "b" : $["start" : [ "1", ]],
        ];
    DUMP (Runlevel::isDirty ());
    Runlevel::services = $[
        "a" : $["dirty" : false ],
        "b" : $["start" : [ "1", ],"dirty" : true ],
        ];
    DUMP (Runlevel::isDirty ());
}
