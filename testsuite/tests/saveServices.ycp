/**
 * YaST2 test case for runlevel editor
 *
 * tests function saveServices
 */
{
    include "runlevel/services.ycp";
    include "testsuite.ycp";

    map services = $[];
    map start_variables_all = $[];

    TEST (``(saveServices ()), [] , 0);

    services = $[
        "a" : $[
            "dirty" : false,
            "start" : ["1", "2", "3", ],
            ],
        "b" : $[
            "dirty" : false,
            "start" : ["1", "2", "3", ],
            ],
        ];
    TEST (``(saveServices ()), [] , 0);

    services = $[
        "a" : $[
            "dirty" : true,
            "start" : ["1", "2", "3", ],
            ],
        "b" : $[
            "dirty" : true,
            "start" : ["1", "2", "3", ],
            ],
        ];
    TEST (``(saveServices ()), [] , 0);

    services = $[
        "a" : $[
            "dirty" : true,
            "start" : ["1", "2", "3", ],
            "defstart" : ["1", "2", "S", "B" ],
            ],
        "b" : $[
            "dirty" : true,
            "start" : ["1", "2", "3", ],
            "defstart" : ["1", "2", "S", "b" ],
            ],
        ];
    TEST (``(saveServices ()), [] , 0);

    start_variables_all = $[
        "a" : "START_a",
        "b" : "START_b",
        ];
    TEST (``(saveServices ()), [] , 0);

    services = $[
        "a" : $[
            "dirty" : true,
            "start" : [],
            "defstart" : ["1", "2", "S", ],
            ],
        "b" : $[
            "dirty" : true,
            "start" : [],
            "defstart" : ["1", "2", "S", ],
            ],
        ];
    start_variables_all = $[
        "a" : "START_a",
        ];
    TEST (``(saveServices ()), [] , 0);
}
