
{
    include "testsuite.ycp";
    import "Runlevel";

    map READ = $[];
    map EXEC = $[];

    EXEC = $[ "target" : $[ "bash" : 3 ] ];
    READ = $[
        "init" : $[
            "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ "0", "1", "3" ],
                        "stop" : [ "4", "5", "6" ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
                ]
            ]
        ];

    TEST (``(Runlevel::ServiceFullInfo ("aaa")), [ READ, $[], EXEC ], nil);
}
