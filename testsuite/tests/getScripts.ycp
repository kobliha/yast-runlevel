/**
 * YaST2 test case for runlevel editor
 *
 * tests function getScripts
 */
{
    import "Runlevel";
    import "Progress";
    include "testsuite.ycp";

    Progress::off ();

    map READ = $[
        "init" : $[
            "scripts" : $[
                "runlevels" : $[
                    "aaa" : $[
                        "start" : [ "0", "1", "3" ],
                        "stop" : [ "4", "5", "6" ],
                        ],
                    "bbb" : $[
                        "start" : [ "0", "1", "2" ],
                        "stop" : [ "5", "6" ],
                        ],
                    ],
                "comments" : $[
                    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
                        ],
                    "bbb" : $[
                        "defstart" : [ "0", "1", "2" ],
                        "defstop" : [ "5", "6" ],
                        ],
                    ],
		"runlevel_list" : [ "0", "1", "2", "3", "5", "6", "S", ],
		"current_runlevel" : "5",
		"default_runlevel" : "5",
                ]
            ]
        ];

    TEST (``(Runlevel::Read ()), [READ] , 0);
    DUMP (Runlevel::services);

    READ = $[
        "init" : $[
            "scripts" : $[
                "runlevels" : $[
                    "aaa" : $[
                        "start" : [ "0", "1", "3" ],
                        "stop" : [ "4", "5", "6" ],
                        ],
                    "ccc" : $[
                        "start" : [ "0", "1", "2" ],
                        "stop" : [ "5", "6" ],
                        ],
                    ],
                "comments" : $[
                    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
                        ],
                    "bbb" : $[
                        "defstart" : [ "0", "1", "2" ],
                        "defstop" : [ "5", "6" ],
                        ],
                    ],
		"runlevel_list" : [ "0", "1", "2", "3", "5", "6", "S", ],
		"current_runlevel" : "5",
		"default_runlevel" : "5",
                ]
            ]
        ];

    TEST (``(Runlevel::Read ()), [READ] , 0);
    DUMP (Runlevel::services);

    Progress::on ();
}
