{
    include "testsuite.ycp";
    import "Runlevel";

    map READ = $[];
    map EXEC = $[];

    // service does not exist
    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ "0", "1", "3" ],
                        "stop" : [ "4", "5", "6" ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : false,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "disable")), [ READ, $[], EXEC ], nil);

    // service exists and may run
    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ "0", "1", "3" ],
                        "stop" : [ "4", "5", "6" ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "disable")), [ READ, $[], EXEC ], nil);

    // service exists and does not run
    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ ],
                        "stop" : [ ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "disable")), [ READ, $[], EXEC ], nil);

    // service exists and does not run
    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ ],
                        "stop" : [ ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "enable")), [ READ, $[], EXEC ], nil);

    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ "1" ],
                        "stop" : [ ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "enable")), [ READ, $[], EXEC ], nil);

    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ "1" ],
                        "stop" : [ ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "default")), [ READ, $[], EXEC ], nil);

    READ = $[ "init" : $[ "scripts" : $[
                "runlevel" : $[
		    "aaa" : $[
                        "start" : [ "0", "1", "3" ],
                        "stop" : [ ],
			]
                    ],
                "comment" : $[
		    "aaa" : $[
                        "defstart" : [ "0", "1", "3" ],
                        "defstop" : [ "4", "5", "6" ],
			]
                    ],
		"exists" : true,
	] ] ];
    EXEC = $[ "target" : $[ "bash" : 0 ]];
    TEST (``(Runlevel::ServiceAdjust ("aaa", "default")), [ READ, $[], EXEC ], nil);

}
