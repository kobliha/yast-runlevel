{
    import "Runlevel";
    import "Progress";

    list args = Args ();
    map settings = select (Args (), 0, $[]);

    Progress::off ();
    Runlevel::Read ();
    Runlevel::Import (settings);
    Runlevel::Write ();
    Progress::on ();
    return true;
}
