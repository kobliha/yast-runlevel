{
    import "RunlevelEd";
    import "Progress";

    list args = WFM::Args ();
    map settings = select (WFM::Args (), 0, $[]);

    Progress::off ();
    RunlevelEd::Read ();
    RunlevelEd::Import (settings);
    RunlevelEd::Write ();
    Progress::on ();
    return true;
}
